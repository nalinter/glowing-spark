pipeline{
    agent any
    stages{
        stage('Code - Quality'){
            steps{
                script{
                    echo "Getting the commit hashes"
                    sh "curr_dir=`pwd`"  
                    sh "cd ${env.WORKSPACE}"
                }      
            }
        }
        stage('Example'){
            when{
                equals expected : "FAILURE" ,actual :currentBuild.result
            }
            steps{
                echo "world"
            }
        }
    }
}



